package Array;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] mailArray = {"jpp@sfr.fr", "tom@gmail.com", "fred@sfr.fr", "victor@sfr.fr", "chris@sfr.fr", "robber@orange.fr", "paul@sfr.fr", "lise@gmail.com", "thierry@isitech.fr", "marie@isitech.fr"};
		int gmail = 0;
		int gmailTT = 0;
		 for (int x = 0; x < 10 ; x = x+1)
		 {
			 boolean var1 = false;
			 var1 = mailArray[x].endsWith("@gmail.com");
			 if (var1 = true) {
				 gmail = gmail + 1;
			 }
		 }
		 boolean var2 = mailArray[0].endsWith("@gmail.com");
		 gmailTT = (gmail / 10) * 100;
		 System.out.println(var2);
		 System.out.println(gmail);
	}

}